<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="admin/tool/bulkreset/db" VERSION="20190725" COMMENT="XMLDB file for Moodle tool/bulkreset"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
    <TABLES>
        <TABLE NAME="tool_bulkreset_schedules" COMMENT="Bulk reset schedules">
            <FIELDS>
                <FIELD NAME="id" SEQUENCE="true" TYPE="int" NOTNULL="true" LENGTH="11" COMMENT="Schedule ID." />
                <FIELD NAME="starttime" SEQUENCE="false" TYPE="int" NOTNULL="true" DEFAULT="0" LENGTH="11" COMMENT="Schedule start timestamp." />
                <FIELD NAME="status" SEQUENCE="false" TYPE="int" NOTNULL="true" DEFAULT="0" LENGTH="11" COMMENT="Schedule status." />
                <FIELD NAME="data" SEQUENCE="false" TYPE="text" NOTNULL="false" COMMENT="Schedule data in JSON format." />
                <FIELD NAME="result" SEQUENCE="false" TYPE="text" NOTNULL="false" COMMENT="SChedule result in JSON format." />
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id" />
            </KEYS>
            <INDEXES>
                <INDEX NAME="status_idx" UNIQUE="false" FIELDS="status" />
            </INDEXES>
        </TABLE>
    </TABLES>
</XMLDB>
